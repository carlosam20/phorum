{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-var": "error",
        "arrow-spacing": "error",
        "no-unused-vars": "warn"
    },
    "globals": {
        "$": true,
        "Mustache":true,
        "swal":true,
        "Cookies":true 
      }

      
      
}
